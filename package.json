{
  "name": "xlayers",
  "version": "1.0.0-beta.5",
  "scripts": {
    "ng": "ng",
    "--dev--": "",
    "start": "ng serve --configuration hmr",
    "prestart": "npm run build.all.libraries",
    "--build--": "",
    "app.build.dev": "ng build",
    "app.build.prod": "ng build --prod",
    "app.build.stamp": "bash scripts/stamp-build.bash",
    "build.all.libraries": "node ./scripts/build-all-libraries.js",
    "build": "run-s -l build.all.libraries app.build.prod app.build.stamp",
    "--test--": "",
    "test": "jest",
    "test.update": "jest --updateSnapshot tests/integration",
    "test.ci": "jest --ci --detectOpenHandles",
    "cy.open": "cypress open",
    "cy.run": "cypress run",
    "e2e": "start-server-and-test start http-get://localhost:4200 cy.run",
    "--lint--": "",
    "lint": "ng lint",
    "lint.fix": "ng lint --fix",
    "--deploy--": "",
    "deploy.prod": "firebase deploy --only hosting",
    "deploy.prod.ci.setup": "firebase use $PROJECT_ID --token $FIREBASE_TOKEN",
    "deploy.prod.ci": "firebase deploy --only hosting -m 'CI deploy' --token $FIREBASE_TOKEN",
    "deploy.ci": "run-s -l deploy.prod.ci.setup deploy.prod.ci",
    "deploy": "run-s -l lint test build deploy.prod",
    "--release--": "",
    "release": "release-it --config .release-it.json",
    "--misc--": "",
    "quality": "run-s -l lint test",
    "changelog": "conventional-changelog -p angular -u | tail -n +3",
    "changelog.write": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "postchangelog.write": "node scripts/clean-changelog.js",
    "changelog.full": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "postchangelog.full": "node scripts/clean-changelog.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/router": "^9.0.0",
    "@angular/service-worker": "^9.0.0",
    "@ngx-translate/core": "^12.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@ngxs/router-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "@stackblitz/sdk": "^1.3.0",
    "big-integer": "^1.6.48",
    "buffer": "^5.4.3",
    "core-js": "^3.6.4",
    "file-saver": "^2.0.0",
    "highlight.js": "^9.18.1",
    "jest-junit": "^10.0.0",
    "jszip": "^3.2.2",
    "ngx-color": "^4.1.1",
    "ngx-highlightjs": "^4.0.2",
    "param-case": "^3.0.3",
    "pascal-case": "^3.1.1",
    "rxjs": "^6.5.4",
    "start-server-and-test": "^1.10.8",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular-devkit/build-ng-packagr": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@ngxs/logger-plugin": "^3.6.2",
    "@testing-library/cypress": "^5.3.0",
    "@types/cypress": "^1.1.3",
    "@types/file-saver": "^2.0.0",
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "^25.1.2",
    "@types/node": "^12.11.1",
    "@types/sketchapp": "^56.0.0",
    "codelyzer": "^5.2.1",
    "conventional-changelog-cli": "^2.0.31",
    "cypress-file-upload": "^3.5.3",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-preset-angular": "^8.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "ora": "^4.0.3",
    "puppeteer": "^2.1.1",
    "release-it": "^12.4.3",
    "speed-measure-webpack-plugin": "^1.3.0",
    "terser": "^4.6.3",
    "ts-jest": "^25.2.0",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "typescript": "3.7.5",
    "wepback": "^1.0.0"
  }
}
